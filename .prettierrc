{
    "plugins": ["prettier-plugin-solidity"],
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": false,
    "bracketSpacing": false,
    "overrides": [
        {
            "files": "*.sol",
            "options": {
                "parser": "solidity-parse"
            }
        }
    ]
}
